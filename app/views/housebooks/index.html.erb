<p id="notice"><%= notice %></p>

<h1>Housebooks</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Use</th>
      <th>Payment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @housebooks.each do |housebook| %>
      <tr>
        <td><%= housebook.Date %></td>
        <td><%= housebook.Use %></td>
        <td><%= housebook.Payment %></td>
        <td><%= link_to 'Show', housebook %></td>
        <td><%= link_to 'Edit', edit_housebook_path(housebook) %></td>
        <td><%= link_to 'Destroy', housebook, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Housebook', new_housebook_path %>
